{
  "name": "frontend",
  "version": "1.0.0",
  "description": "A modern audio file upload dashboard with API integration",
  "main": "public/index.html",
  "scripts": {
    "start": "npx http-server public -p 3000 -o",
    "dev": "npx http-server public -p 3000 -o",
    "build": "npm run minify-css && npm run minify-js",
    "minify-css": "npx clean-css-cli src/css/*.css -o public/css/bundle.min.css",
    "minify-js": "npx terser src/js/*.js -o public/js/bundle.min.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "npx eslint src/js/",
    "format": "npx prettier --write src/"
  },
  "keywords": [
    "audio",
    "upload",
    "dashboard",
    "file-upload",
    "web-app"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.0"
  },
  "devDependencies": {
    "http-server": "^14.1.1",
    "clean-css-cli": "^5.6.2",
    "terser": "^5.19.4",
    "eslint": "^8.48.0",
    "prettier": "^3.0.3"
  }
}